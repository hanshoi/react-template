{
  "name": "testing-example",
  "version": "0.0.1",
  "description": "Example of testing react.",
  "author": "Antti Ruotsalainen",
  "license": "MIT",
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive app/test/*"
