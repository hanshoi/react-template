    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "expect": "^1.14.0",
    "gulp": "^3.9.0",
    "gulp-livereload": "^3.8.1",
    "gulp-notify": "^2.2.0",
    "gulp-size": "^2.0.0",
    "gulp-util": "^3.0.7",
    "jsdom": "^8.0.2",
    "livereload": "^0.4.1",
    "mocha": "^2.4.5",
    "node-inspector": "^0.12.5",
    "react-addons-test-utils": "^0.14.6",
    "underscore": "^1.8.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "vars": {
    "build_folder": "app",
    "develop": true,
    "app_bundle": "build.js",
    "app_entrypoint": "./app/app.js",
