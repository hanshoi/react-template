{
  "name": "isomorphic-example",
  "version": "0.0.1",
  "description": "Isomorphic web application using react, alt and express",
  "author": "Antti Ruotsalainen",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --debug -w build build/server.js"
  },
  "dependencies": {
    "react": "^0.14.x",
    "react-dom": "^0.14.x",
    "express": "^4.13.x",
    "express-handlebars": "^3.0.x"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "gulp": "^3.9.0",
    "gulp-clean": "^0.3.1",
    "gulp-nodemon": "^2.0.6",
    "gulp-notify": "^2.2.0",
    "gulp-size": "^2.0.0",
    "gulp-util": "^3.0.7",
    "nodemon": "^1.8.1",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "vars": {
    "client_dependencies": [
      "react",
      "react-dom",
      "alt"
    ],
    "develop": true,
    "js_out": "build",
    "js_files": "app/**/*.js",
    "html_files": "app/**/*.handlebars",
    "client_bundle": "client.build.js",
    "vendor_bundle": "vendor.build.js",
    "client_entrypoint": "./app/client.js",
    "server_entrypoint": "./build/server.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "ignore": "node_modules"
  }
}
